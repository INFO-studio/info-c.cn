const ctx = canvas.value.getContext('2d')
ctx.beginPath()
ctx.rect(80, 20, 70, 150)         // 创建矩形路径，(80, 20) 为起始点，宽70高150
ctx.stroke()

ctx.beginPath()
ctx.strokeStyle = 'purple'
ctx.lineWidth = 5
ctx.strokeRect(50, 90, 120, 30)   // 创建矩形路径并绘制线段

ctx.beginPath()
ctx.fillStyle = '#ff6600cc'
ctx.fillRect(20, 30, 80, 80)      // 创建矩形路径并填充面积